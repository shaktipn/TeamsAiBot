{
  "name": "@teamsaibot/fe",
  "version": "9999.9999.9999",
  "private": true,
  "description": "React FE app for TeamsAIBot.",
  "scripts": {
    "build": "npm -w @teamsaibot/react-app run build",
    "start": "npm -w @teamsaibot/react-app run dev",
    "format": "biome format --write .",
    "format-check": "biome check . --formatter-enabled=true --linter-enabled=false",
    "lint": "biome check --write .",
    "lint-check": "biome check . --formatter-enabled=false --linter-enabled=true",
    "lint-fix": "biome check --fix --unsafe .",
    "preview": "npm -w @teamsaibot/react-app run preview"
  },
  "workspaces": ["packages/*"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SuryaDigital/None"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@vitejs/plugin-react-swc": "3.9.0",
    "react-dev-utils": "12.0.1"
  },
  "engines": {
    "node": ">=20.18.1 <21"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-arm64-gnu": "4.40.2",
    "@swc/core-linux-arm64-musl": "1.11.29"
  }
}
